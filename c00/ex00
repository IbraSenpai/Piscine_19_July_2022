#include <stdio.h>
#include <string.h>

char	*ft_strcpy(char *dest, char *src)
{
	int	i;

	i = 0;
	while (src[i])
	{
		dest[i] = src[i];
		i++;
	}
	return (dest);
}

int main() {
  char str1[20] = "C programming";
  char str2[20];

  // copying str1 to str2
  ft_strcpy(str2, str1);

  puts(str2); // C programming

  return 0;
}
